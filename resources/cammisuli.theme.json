{
  "name": "cammisuli theme",
  "dark": true,
  "author": "jonathan cammisuli",
  "editorScheme": "/cammisuli.xml",
  "colors": {
    "headerbackground": "#21252c",
    "border": "#2d313b",
    "black": "#21252c",
    "button": "#6a6a6a"
  },
  "ui": {
    "*": {
      "background": "headerbackground"
    },
    "Borders": {
      "color": "border",
      "ContrastBorderColor": "border"
    },
    "EditorTabs": {
      "background": "headerbackground",
      "underlineHeight": "1",
      "underlinedTabBackground": "headerbackground",
      "hoverBackground": "headerbackground"

    },
    "Panel.background": "black",
    "Button.background": "black",
    "CheckBox.background": "black",
    "TextArea.background": "black",
    "Tree": {
      "rowHeight": 30,
      "background": "#1c2026"
    },
    "ToolWindow": {
      "Header.inactiveBackground": "headerbackground",
      "Header.background": "headerbackground",
      "HeaderTab.underlineHeight": "1"
    }
  }
}