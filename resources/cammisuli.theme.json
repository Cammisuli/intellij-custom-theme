{
  "name": "cammisuli theme",
  "dark": true,
  "author": "jonathan cammisuli",
  "editorScheme": "/cammisuli.xml",
  "colors": {
    "borderColor": "#333333",
    "tabColor": "#333333",
    "inactive": "#444444",
    "selected": "#6a6a6a",
    "button": "#6a6a6a",
    "white": "#f1f1f1",
    "background": "#333333"
  },
  "ui": {
    "*": {
      "foreground": "#f1f1f1",
      "background": "#333333",
      "borderColor": "borderColor",
      "selectionBackground": "selected",
      "selectionForeground": "#fdf6e3",
      "selectionInactiveBackground": "inactive",
      "selectionBackgroundInactive": "inactive",
      "separatorColor": "borderColor",
      "lineSeparatorColor": "borderColor",
      "inactiveBackground": "inactive"
    },


    "ActionButton": {
      "hoverBorderColor": "button",
      "pressedBackground": "button",
      "pressedBorderColor": "button"
    },

    "Borders": {
      "color": "borderColor",
      "ContrastBorderColor": "borderColor"
    },

    "Button": {
      "startBackground": "button",
      "endBackground": "button",
      "default": {
        "foreground": "#fdf6e3",
        "startBackground": "button",
        "endBackground": "button",
        "startBorderColor": "button",
        "endBorderColor": "button"
      }
    },


    "DragAndDrop": {
      "areaForeground": "#bac8cb",
      "areaBackground": "#1E2A3E",
      "areaBorderColor": "borderColor"
    },

    "EditorPane.inactiveBackground": "inactive",

    "EditorTabs": {
      "background": "#444444",
      "underlinedTabBackground": "background",
      "underlineColor": "background"
    },



    "Group.disabledSeparatorColor": "borderColor",

    "InplaceRefactoringPopup.borderColor": "borderColor",


    "Label.errorForeground": "#af304a",

    "NavBar.borderColor": "borderColor",

    "Notification": {
      "background": "#1E2A3E",
      "borderColor": "#586E75",
      "errorForeground": "#bac8cb",
      "errorBackground": "#4d232e",
      "errorBorderColor": "#802e44",
      "MoreButton": {
        "background": "#1E2A3E",
        "foreground": "#bac8cb",
        "innerBorderColor": "borderColor"
      },
      "ToolWindow": {
        "informativeForeground": "#bac8cb",
        "informativeBackground": "button",
        "informativeBorderColor": "borderColor",
        "warningForeground": "#bac8cb",
        "warningBackground": "#8d8d55",
        "warningBorderColor": "borderColor",
        "errorForeground": "#bac8cb",
        "errorBackground": "#802d43",
        "errorBorderColor": "#4d1c2b"
      }
    },

    "PopupMenu": {
      "borderWidth": 1,
      "borderInsets": "1,1,1,1"
    },

    "ProgressBar": {
      "trackColor": "#1E2A3E",
      "progressColor": "#029098",
      "indeterminateStartColor": "#02858e",
      "indeterminateEndColor": "#2cb0a7",
      "failedColor": "#ff4262",
      "failedEndColor": "#a12a3e",
      "passedColor": "#2cb0a7",
      "passedEndColor": "#026d76"
    },

    "SearchEverywhere": {
      "Header.background": "#1E2A3E",
      "Tab": {
        "selectedForeground": "#bac8cb",
        "selectedBackground": "button"
      },
      "SearchField": {
        "background": "#172030",
        "borderColor": "borderColor"
      },
      "Advertiser.foreground": "#bac8cb"
    },

    "SearchMatch": {
      "startBackground": "#e1e797",
      "endBackground": "#e1e797"
    },

    "SpeedSearch": {
      "foreground": "#bac8cb",
      "borderColor": "borderColor",
      "background": "#1E2A3E",
      "errorForeground": "#e0555e"
    },

    "SplitPane.background": "#172030",

    "StatusBar.borderColor": "borderColor",

    "Tree.rowHeight": 22,

    "ToolWindow": {
      "Header": {
        "background": "background",
        "inactiveBackground": "background"
      },
      "HeaderTab": {
        "hoverBackground": "background",
        "selectedInactiveBackground": "background",
        "hoverInactiveBackground": "background"
      }
    }
  }
}